{
  "version": 2,
  "builds": [
    {
      "src": "vercel-api-version.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/vercel-api-version.js"
    }
  ],
  "functions": {
    "vercel-api-version.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
} 